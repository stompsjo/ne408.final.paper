@inproceedings{nasa-propulsion,
	address = {Bordeaux, France},
	title = {{NASA}'s {Launch} {Propulsion} {Systems} {Technology} {Roadmap}},
	url = {https://ntrs.nasa.gov/search.jsp?R=20120014957},
	abstract = {Safe, reliable, and affordable access to low-Earth  orbit is necessary for all of the United States  space endeavors. In 2010, NASA s Office of the Chief Technologist commissioned 14 teams to develop technology roadmaps that could be used to guide the Agency s and US technology investment decisions for the next few decades. The Launch Propulsion Systems Technology Area  team was tasked to address the propulsion technology challenges for access to LEO. The developed LPSTA roadmap addresses technologies that enhance existing solid or liquid propulsion technologies and their related ancillary systems or significantly advance the technology readiness level  of less mature systems like airbreathing, unconventional, and other launch technologies. In developing this roadmap, the LPSTA team consulted previous NASA, military, and industry studies as well as subject matter experts to develop their assessment of this field, which has fundamental technological and strategic impacts for US space capabilities.},
	urldate = {2020-05-04},
	author = {{Paul K.~McConnaughey} and {Mark G.~Femminineo} and {Syri J. Koelfgen} and {Roger A.~Lepsch} and {Richard M.~Ryan} and {Steven A.~Taylor}},
	month = may,
